{
  "name": "cash-track",
  "version": "1.0.0",
  "description": "Travel Expense Tracker",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm runt start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \" nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client npm run start",
    "post-install": "cd client && npm run start",
    "build": "cd client && npm run build",
    "preinstall": "cd client && npm install",
    "heroku-postbuild": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smalagadev/Cash-Track.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smalagadev/Cash-Track/issues"
  },
  "homepage": "https://github.com/smalagadev/Cash-Track#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.7",
    "path": "^0.12.7"
  }
}
